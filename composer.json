{
  "name": "code-tool/doctrine-dbal-jaeger",
  "license": "MIT",
  "require": {
    "php": "^7.2 || ^8",
    "doctrine/dbal": "^2.8",
    "code-tool/jaeger-client-php": "^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Doctrine\\DBAL\\Jaeger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Doctrine\\DBAL\\Jaeger\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
